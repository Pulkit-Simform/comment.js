#cjs-src
  user-select none
  code
    user-select text
    background #222
    display block
    color #F8F8F2
    font-family Menlo, Consolas, Monaco, "Lucida Console", monospace
    white-space pre
    padding-left 80px
    line-height 1.2
    &:hover
      background #444
    &:hover .ln
    color #fff
    // linenum
    .ln
      position absolute
      left 0
      color $hi-line-text-color
      width 50px
      text-align right
      user-select none
    // keyword
    b
      color $hi-keyword
      font-weight normal
    // string
    i, i *, i .d
      color $hi-string
      font-style normal
    // operator
    .s
      color $hi-operator
    // number
    .d
      color $hi-number
    // comment
    .c, .c *
      color $hi-comment

// selection color
selection(rgba(69, 69, 69, 0.99), inherit, '#cjs-src')
selection(rgba(69, 69, 69, 0.99), inherit, '#cjs-src code')
selection(rgba(69, 69, 69, 0.99), inherit, '#cjs-src code *')
